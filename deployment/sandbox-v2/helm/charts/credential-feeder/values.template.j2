component: credential-feeder
part-of: mosip-idrepo
activeProfileEnv: mz
springConfigLabel: {{ config_repo.version }} 
configServerUrl: {{ config_server.internal_url }}
artifactoryUrl: {{ artifactory_url }} 
exportLogs: enabled  # Let filebeat export the logs
probe:
  initialDelay: 120  # Seconds
  period: 30 # Seconds
imagePullPolicy: Always
isGlowroot: {{ is_glowroot }} 

dockerHub:
  private: {{ docker.hub.private }}  
  keyname: {{ docker.hub.keyname }}

generators:
  cred:
    name: {{ versions.services.idrepo.feeder.keys()[0] }}
    dockerImage: {{ versions.services.idrepo.feeder.values()[0] }}
